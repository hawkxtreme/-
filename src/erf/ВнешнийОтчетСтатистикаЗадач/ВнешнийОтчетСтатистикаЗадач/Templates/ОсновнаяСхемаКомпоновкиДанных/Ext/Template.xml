			<dataPath>КоличествоОткрытыхЗадач</dataPath>
			<field>КоличествоОткрытыхЗадач</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Количество открытых задач</v8:content>
